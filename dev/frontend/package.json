{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint-check": "eslint ./src",
    "lint-fix": "eslint --fix ./src",
    "lint-check-staged": "eslint $(git diff --cached --name-only --diff-filter=ACM | sed 's:^dev/frontend/react_app/::')",
    "lint-fix-staged": "eslint --fix $(git diff --cached --name-only --diff-filter=ACM | sed 's:^dev/frontend/react_app/::')",
    "format-write": "prettier --write \"**/*.{ts,tsx}\"",
    "format-check": "prettier --check \"**/*.{ts,tsx}\"",
    "format-check-staged": "prettier --check $(git diff --cached --name-only --diff-filter=ACM | sed 's:^dev/frontend/react_app/::')",
    "format-write-staged": "prettier --write $(git diff --cached --name-only --diff-filter=ACM | sed 's:^dev/frontend/react_app/::')",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/node": "^24.0.7",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "eslint": "^9.22.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "prettier": "^3.6.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1"
  }
}
